<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="members">

	<select id="loginchk" parameterType="String" resultType="com.ict.edu09.vo.MembersVO">
		select * from members where m_id=#{m_id}
	
	
	
	</select>
	
	
	<insert id="join" parameterType="com.ict.edu09.vo.MembersVO">
	
		insert into members(m_id, m_pw, m_name, m_addr, m_addr2, m_email, m_phone)
		values(#{m_id}, #{m_pw},#{m_name}, #{m_addr},#{m_addr2}, #{m_email},#{ m_phone})
	
	
	</insert>



</mapper>